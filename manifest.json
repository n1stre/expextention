{
  "manifest_version": 2,
  "name": "expextention",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "app/dist/bundle.js"
      ]
    }
  ],
  "background": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "app/dist/background.bundle.js"
      ]
    }
  ]
}